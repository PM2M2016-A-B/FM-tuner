    "moment": "^2.10.6"
  },
  "devDependencies": {
    "babel": "^5.8.34",
    "babel-eslint": "^4.1.5",
    "clarify": "^1.0.5",
    "dependency-check": "^2.5.1",
    "mocha": "^2.3.4",
    "must": "^0.13.1",
    "nyc": "^3.2.2",
    "source-map-support": "^0.3.3",
    "standard": "^5.4.1",
    "trace": "^2.0.1"
  },
  "scripts": {
    "build": "babel --source-maps --out-dir=dist/ src/",
